<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:drawable="@drawable/firebase_logo"

        />

    <item>

        <shape android:shape="rectangle">
            <solid
                android:color="@color/opaque"
                />
            <stroke
                android:color="@color/colorPrimaryTransparent"
                android:width="2dp"
                />
        </shape>

        <padding
            android:top="0dp"
            android:bottom="0dp"
            android:left="0dp"
            android:right="0dp"
            />

    </item>



</layer-list>